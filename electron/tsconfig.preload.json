{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"lib": ["ES2020"],
		"outDir": "./dist/electron",
		"noEmit": false,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"types": ["node"]
	},
	"include": ["preload.ts"],
	"exclude": ["node_modules", "dist"]
}


